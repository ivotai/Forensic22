<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.unicorn.forensic2.ui.other.TitleBar
        android:id="@+id/titleBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/ivFidzyzsNew"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:scaleType="fitXY"
        android:src="@mipmap/upload_picture"
        app:layout_constraintBottom_toBottomOf="@+id/ivFidzczsNew"
        app:layout_constraintEnd_toStartOf="@+id/ivFidzczsNew"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ivFidzczsNew" />

    <TextView
        android:id="@+id/tvFidzyzsNew"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="选择执业证书"
        android:textColor="@color/colorPrimary"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="@+id/ivFidzyzsNew"
        app:layout_constraintStart_toStartOf="@+id/ivFidzyzsNew"
        app:layout_constraintTop_toBottomOf="@+id/ivFidzyzsNew" />

    <ImageView
        android:id="@+id/ivFidzczsNew"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="32dp"
        android:scaleType="fitXY"
        android:src="@mipmap/upload_picture"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/ivFidzyzsNew"
        app:layout_constraintTop_toBottomOf="@+id/titleBar" />

    <TextView
        android:id="@+id/tvFidzczsNew"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="选择职称证书"
        android:textColor="@color/colorPrimary"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="@+id/ivFidzczsNew"
        app:layout_constraintHorizontal_bias="0.857"
        app:layout_constraintStart_toStartOf="@+id/ivFidzczsNew"
        app:layout_constraintTop_toTopOf="@+id/tvFidzyzsNew" />

</androidx.constraintlayout.widget.ConstraintLayout>